{
  "name": "tinder-modern",
  "version": "1.1.1",
  "description": "Node.js bindings to Tinder API",
  "main": "dist/bundle.js",
  "module": "dist/bundle.es.js",
  "repository": "https://github.com/wasd171/tinder-modern.git",
  "author": "Konstantin Nesterov <wasd171@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.4.0",
    "eslint": "^3.19.0",
    "eslint-config-prettier": "^1.7.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.0.1",
    "eslint-plugin-react": "^6.10.3",
    "flow-bin": "^0.44.2",
    "flow-typed": "^2.1.2",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "prettier": "^1.2.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1"
  },
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && rollup -c rollup-configs/cjs.js && rollup -c rollup-configs/es.js",
    "precommit": "lint-staged && npm run build && git add dist/*.js",
    "flow": "flow check-contents <",
    "postinstall": "flow-typed install"
  },
  "lint-staged": {
    "{src,rollup-configs}/*.js": [
      "eslint --fix",
      "npm run flow",
      "git add"
    ]
  },
  "dependencies": {
    "form-data": "^2.1.4",
    "node-fetch": "^1.6.3"
  }
}
