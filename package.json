  "types": "dist/index.d.ts",
  "repository": "https://github.com/wasd171/tinder-modern.git",
  "author": "Konstantin Nesterov <wasd171@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/form-data": "^0.0.33",
    "@types/node-fetch": "^1.6.7",
    "husky": "^0.14.0",
    "lint-staged": "^4.0.0",
    "prettier": "^1.4.4",
    "rimraf": "^2.6.1",
    "tslint": "^5.4.3",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.3.4"
